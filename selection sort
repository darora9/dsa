#include<iostream>

using namespace std;

int main(){
int arr[5]={4,5,87,67,99};
int val;

for(int i=0;i<5;i++){
    for(int j=i+1;j<5;j++){
        if(arr[i]>arr[j]){
            val=arr[i];
            arr[i]=arr[j];
            arr[j]=val;
        }
    }
}
for(int x=0;x<5;x++){
    cout<<arr[x]<<endl;
}

return 0;
}
